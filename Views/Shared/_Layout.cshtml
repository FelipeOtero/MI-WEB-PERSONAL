<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - MI_WEB_PERSONAL</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/MI_WEB_PERSONAL.styles.css" asp-append-version="true" />
</head>
<body>
    
   <nav class="navbar navbar-expand-lg navbar-light bg-light fixed-top">
    <a class="navbar-brand" href="#">Portafolio</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNav">
      <ul class="navbar-nav ml-auto">
        <li class="nav-item">
          <a class="nav-link" href="@Url.Action("Index", "Home")">Sobre mi</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="@Url.Action("Proyectos", "Home")">Proyectos</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="@Url.Action("Cursos", "Home")">Cursos</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="@Url.Action("Idiomas", "Home")">Idiomas</a>
        </li>
      </ul>
    </div>
  </nav>

    <main role="main" class="pb-3">
        @RenderBody()
    </main>
    
  <footer class="footer">
    <p>&copy; 2024 Felipe Otero. Todos los derechos reservados.</p>
  </footer>
    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
